unit Unit11;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, StdCtrls, ExtCtrls;

type
  TForm11 = class(TForm)
    Label1: TLabel;
    RadioGroup1: TRadioGroup;
    Button1: TButton;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form11: TForm11;
   a2:real;
implementation

uses Unit12;

{$R *.dfm}

procedure TForm11.Button1Click(Sender: TObject);
begin
case RadioGroup1.ItemIndex of
  0:
  begin
   a2:=0;
  Form12.show;
  Form11.Close;
  end;
  1:
  begin
     a2:=0;
  Form12.show;
  Form11.Close;
  end;
  2:
  begin
    a2:=0;
  Form12.show;
  Form11.Close;
  end;
  3:
  begin
         a2:=0;
  Form12.show;
  Form11.Close;
  end;
  4:

  begin
      a2:=10;
  Form12.show;
  Form11.Close;
  end;

end;
end;



end.
